{
  "name": "@learnpack/markdown",
  "version": "0.0.1",
  "description": "Collection of usefull components for react.js applications around the BreatheCode ecosystem",
  "main": "dist/index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "dev": "webpack --config webpack.config.js --mode development",
    "build": "webpack --config webpack.config.js --mode production",
    "start": "start-storybook -p 3000 --ci",
    "build-storybook": "build-storybook",
    "deploy": "build-storybook && node deploy-github.js",
    "publish-to-npm": "webpack --config webpack.config.js --mode production && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/learnpack/markdown.git"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "keywords": [
    "React",
    "BreatheCode"
  ],
  "author": "Alejandro Sanchez",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/learnpack/learnpack/issues"
  },
  "homepage": "https://github.com/learnpack/markdown#readme",
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.14.5",
    "@storybook/addon-actions": "^5.3.21",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-knobs": "^5.3.21",
    "@storybook/addon-links": "^5.3.21",
    "@storybook/addon-notes": "^5.3.21",
    "@storybook/addons": "^5.3.21",
    "@storybook/react": "^5.3.21",
    "babel-loader": "^8.2.2",
    "css-loader": "^0.28.11",
    "isomorphic-style-loader": "^5.3.0",
    "mini-css-extract-plugin": "^0.6.0",
    "style-loader": "^0.21.0",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "prismjs": "^1.24.1",
    "prop-types": "^15.7.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "refractor": "^2.10.1",
    "rehype": "^7.0.0",
    "remark": "^10.0.1",
    "remark-comments": "^1.2.9",
    "remark-emoji": "^2.2.0",
    "remark-react": "^5.0.1",
    "remark-rehype": "^4.0.1",
    "remark-toc": "^7.2.0",
    "remark-variables": "^1.4.9"
  }
}
